import pandas as pd

# Leer el archivo CSV
data = pd.read_csv('archivo.csv')

# Ver las primeras filas del archivo CSV
print(data.head())

# Escribir el contenido en el archivo README.md
with open('README.md', 'a') as readme_file:
    readme_file.write("\n## Contenido del archivo CSV\n")
    readme_file.write(data.to_markdown(index=False))

print("Contenido del archivo CSV agregado al README.md")
